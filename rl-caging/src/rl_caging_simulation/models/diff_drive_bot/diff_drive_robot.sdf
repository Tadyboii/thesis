<?xml version="1.0" ?>
<sdf version="1.8">
    <model name='vehicle_blue' canonical_link='chassis'>
        <pose>0 0 1 0 0 0</pose>   <!--the pose is relative to the world by default-->

        <!--chassis-->
        <link name='chassis'>
            <pose relative_to='__model__'>0 0 0.05 0 0 0</pose>
            <inertial> <!--inertial properties of the link mass, inertia matix-->
                <mass>1000</mass>
                <inertia>
                    <ixx>0.095329</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.381317</iyy>
                    <iyz>0</iyz>
                    <izz>0.476646</izz>
                </inertia>
            </inertial>
            <visual name="base_visual">
                <pose>0 0 0 0 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>0.14</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.3 0.3 0.3 1.0</ambient>
                    <diffuse>0.3 0.3 0.3 1.0</diffuse>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.14</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
            </collision>
        </link>

        <!--Left wheel-->
        <link name='left_wheel'>
            <pose relative_to="chassis">0 0.10 -0.005 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.25</mass>
                <inertia>
                    <ixx>0.043333</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.043333</iyy>
                    <iyz>0</iyz>
                    <izz>0.08</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.033</radius>
                        <length>0.018</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1.0 0.0 0.0 1</ambient>
                    <diffuse>1.0 0.0 0.0 1</diffuse>
                    <specular>1.0 0.0 0.0 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.033</radius>
                        <length>0.018</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--The same as left wheel but with different position-->
        <link name='right_wheel'>
            <pose relative_to="chassis">0 -0.10 -0.005 -1.5707 0 0</pose> <!--angles are in radian-->
            <inertial>
                <mass>0.25</mass>
                <inertia>
                    <ixx>0.043333</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.043333</iyy>
                    <iyz>0</iyz>
                    <izz>0.08</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.033</radius>
                        <length>0.018</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>1.0 0.0 0.0 1</ambient>
                    <diffuse>1.0 0.0 0.0 1</diffuse>
                    <specular>1.0 0.0 0.0 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.033</radius>
                        <length>0.018</length>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--arbitrary frame-->
        <frame name="caster_frame" attached_to='chassis'>
            <pose>0.10 0 -0.025 0 0 0</pose>
        </frame>

        <!--caster wheel-->
        <link name='caster'>
            <pose relative_to='caster_frame'/>
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>0.016</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.016</iyy>
                    <iyz>0</iyz>
                    <izz>0.016</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.013</radius>
                    </sphere>
                </geometry>
                <material>
                    <ambient>0.0 1 0.0 1</ambient>
                    <diffuse>0.0 1 0.0 1</diffuse>
                    <specular>0.0 1 0.0 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.013</radius>
                    </sphere>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.0</mu>
                            <mu2>0.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--rear caster frame-->
        <frame name="rear_caster_frame" attached_to='chassis'>
            <pose>-0.10 0 -0.025 0 0 0</pose>
        </frame>

        <!--rear caster wheel-->
        <link name='rear_caster'>
            <pose relative_to='rear_caster_frame'/>
            <inertial>
                <mass>0.02</mass>
                <inertia>
                    <ixx>0.016</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.016</iyy>
                    <iyz>0</iyz>
                    <izz>0.016</izz>
                </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.013</radius>
                    </sphere>
                </geometry>
                <material>
                    <ambient>0.0 1 0.0 1</ambient>
                    <diffuse>0.0 1 0.0 1</diffuse>
                    <specular>0.0 1 0.0 1</specular>
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.013</radius>
                    </sphere>
                </geometry>
                <surface>
                    <friction>
                        <ode>
                            <mu>0.0</mu>
                            <mu2>0.0</mu2>
                        </ode>
                    </friction>
                </surface>
            </collision>
        </link>

        <!--left wheel joint-->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel'/>
            <parent>chassis</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz> <!--can be descired to any frame or even arbitrary frames-->
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                </limit>
            </axis>
        </joint>

        <!--right wheel joint-->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel'/>
            <parent>chassis</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative infinity-->
                    <upper>1.79769e+308</upper>     <!--positive infinity-->
                </limit>
            </axis>
        </joint>

        <!--caster wheel joint--> <!--pose defult value is the child-->
        <joint name='caster_wheel' type='ball'>
            <parent>chassis</parent>
            <child>caster</child>
            <axis>
                <xyz>0 0 1</xyz>
                <dynamics>
                    <damping>0.1</damping>
                </dynamics>
            </axis>
        </joint>

        <!--rear caster wheel joint-->
        <joint name='rear_caster_wheel' type='ball'>
            <parent>chassis</parent>
            <child>rear_caster</child>
            <axis>
                <xyz>0 0 1</xyz>
                <dynamics>
                    <damping>0.1</damping>
                </dynamics>
            </axis>
        </joint>

        <plugin
            filename="gz-sim-diff-drive-system"
            name="gz::sim::systems::DiffDrive">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <wheel_separation>0.20</wheel_separation>
            <wheel_radius>0.033</wheel_radius>
            <odom_topic></odom_topic>
            <odom_publisher_frequency>0.0</odom_publisher_frequency>
            <topic>cmd_vel</topic>
        </plugin>

        <link name="base_scan">
            <pose relative_to="chassis">0 0 0.05 0 0 0</pose>
            <inertial>
                <inertia>
                <ixx>0.0001</ixx>
                <ixy>0.000</ixy>
                <ixz>0.000</ixz>
                <iyy>0.0001</iyy>
                <iyz>0.000</iyz>
                <izz>0.0001</izz>
                </inertia>
                <mass>0.114</mass>
            </inertial>

            <collision name="lidar_sensor_collision">
                <geometry>
                <cylinder>
                    <radius>0.1016</radius>
                    <length>0.055</length>
                </cylinder>
                </geometry>
            </collision>

            <visual name="lidar_sensor_visual">
                <geometry>
                    <cylinder>
                        <radius>0.1016</radius>
                        <length>0.055</length>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.2 0.2 0.2 1.0</ambient>
                    <diffuse>0.2 0.2 0.2 1.0</diffuse>
                </material>
            </visual>
            <visual name="front_marker">
                <pose>0.06 0 0.035 0 0 0</pose>
                <geometry>
                    <box>
                        <size>0.12 0.020 0.015</size>
                    </box>
                </geometry>
                <material>
                    <ambient>0 0 1 1</ambient>
                    <diffuse>0 0 1 1</diffuse>
                </material>
            </visual>

            <sensor name="hls_lfcd_lds" type="gpu_lidar">
                <always_on>true</always_on>
                <visualize>true</visualize>
                <update_rate>20</update_rate>
                <topic>scan</topic>
                <frame_id>base_scan</frame_id>
                <lidar>
                    <scan>
                        <horizontal>
                        <samples>360</samples>
                        <resolution>1.000000</resolution>
                        <min_angle>0.000000</min_angle>
                        <max_angle>6.280000</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.1</min>
                        <max>7</max>
                        <resolution>0.015000</resolution>
                    </range>
                    <noise>
                        <type>gaussian</type>
                        <mean>0.0</mean>
                        <stddev>0.01</stddev>
                    </noise>
                </lidar>
            </sensor>
        </link>

        <joint name="lidar_joint" type="fixed">
            <parent>chassis</parent>
            <child>base_scan</child>
            <axis>
                <xyz>0 0 1</xyz>
            </axis>
        </joint>

    </model>
</sdf>